{
  "compilerOptions": {
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es6",
    "esModuleInterop": true,
    "sourceMap": true,

    "alwaysStrict": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": false,
    "strictNullChecks": true
  }
}
